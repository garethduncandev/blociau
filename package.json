{
  "name": "blociau",
  "version": "2.0.11",
  "type": "module",
  "homepage": "https://github.com/garethduncandev/blociau#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/garethduncandev/blociau.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build:vite": "tsc && vite build",
    "build": "tsc --build --clean && tsc",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "npm publish --access public",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "format:check": "npx prettier .",
    "format:write": "npx prettier . --write",
    "format": "npm run lint:fix && npm run format:write",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/web-components": "^8.2.9",
    "@storybook/web-components-vite": "^8.2.9",
    "clean-package": "^2.2.0",
    "eslint": "^9.17.0",
    "lit": "^3.2.1",
    "prettier": "3.4.2",
    "storybook": "^8.2.9",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.4.1"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
