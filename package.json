{
  "name": "@garethduncandev/blociau",
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier . --check",
    "prettier:write": "prettier . --write",
    "lint": "eslint .",
    "lint:write": "eslint . --fix",
    "format": "npm run prettier && npm run lint",
    "format:write": "npm run prettier:write && npm run lint:write",
    "prepublish": "npm run build",
    "deploy": "npm publish --access public",
    "deploy:locally": "npm publish --access public --userconfig ~/.npmrc-garethduncandev",
    "whoami": "npm whoami --userconfig ~/.npmrc-garethduncandev"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  },
  "main": "./dist/blociau.umd.cjs",
  "module": "./dist/blociau.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/blociau.js",
      "require": "./dist/blociau.umd.js"
    }
  },
  "files": [
    "dist"
  ]
}
